GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/AFNetworking.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/CLKSingletons.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/MagicalRecord.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/NSString_Ruby.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/NSURL_QueryDictionary.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/PureLayout.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/ReactiveCocoa.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/UIColor_Utilities.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/UIView_Positioning.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "AFNetworking" -framework "CLKSingletons" -framework "MagicalRecord" -framework "NSString_Ruby" -framework "NSURL_QueryDictionary" -framework "PureLayout" -framework "ReactiveCocoa" -framework "UIColor_Utilities" -framework "UIView_Positioning"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods
PODS_ROOT = ${SRCROOT}/Pods